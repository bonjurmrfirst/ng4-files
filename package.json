{
  "name": "angular4-files-upload",
  "version": "1.0.2",
  "description": "Angular4 files upload by click or/and drag and drop",
  "keywords": [
    "Angular4",
    "ng4",
    "file",
    "file upload",
    "drop file",
    "drag and drop upload",
    "angular upload"
  ],
  "author": {
    "name": "Potsepaiev Illia",
    "email": "bonjurmrfirst@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bonjurmrfirst/ng4-files/"
  },
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test:node": "jasmine-ts JASMINE_CONFIG_PATH=./jasmine.tdd.json --fast=true",
    "test": "nodemon -e ts -w ./src -x jasmine-ts JASMINE_CONFIG_PATH=./jasmine.tdd.json  --fast=true",
    "tdd": "nodemon -e ts -w ./src -x jasmine-ts JASMINE_CONFIG_PATH=./jasmine.tdd.json  --fast=true",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "verify": "npm run lint && npm run test:node && npm run build && npm run build:prod",
    "packagr": "ng-packagr -p ng-package.json && cd pack && npm pack"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "core-js": "^2.4.1",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "1.3.0",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "~3.1.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "jasmine-ts": "^0.2.1",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^1.6.0",
    "protractor": "~5.1.2",
    "ts-node": "~3.2.0",
    "tslint": "~5.3.2",
    "typescript": "~2.3.3"
  }
